/**
 * 주어진 빗썸 심볼이 업비트 USDT 마켓에서 거래 가능한지 확인
 */
export function hasUpbitUsdtPair(bithumbSymbol: string): boolean {
  return bithumbSymbol in UPBIT_USDT_MAPPING;
}

/**
 * 빗썸 심볼을 업비트 USDT 심볼로 변환
 */
export function getUpbitUsdtSymbol(bithumbSymbol: string): string | null {
  return UPBIT_USDT_MAPPING[bithumbSymbol] || null;
}
