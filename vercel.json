{
  "functions": {
    "app/api/**/*.ts": {
      "memory": 512,
      "maxDuration": 10
    },
    "app/api/**/*.js": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "crons": [],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=300, stale-while-revalidate=3600"
        }
      ]
    }
  ]
}